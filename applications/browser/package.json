    "cdt-gdb-vscode": "https://open-vsx.org/api/eclipse-cdt/cdt-gdb-vscode/0.0.99/file/eclipse-cdt.cdt-gdb-vscode-0.0.99.vsix",
    "vscode-eslint": "https://github.com/theia-ide/vscode-eslint/releases/download/release%2F2.0.15/vscode-eslint-2.0.15.vsix",
    "vscode-php-intellisense": "https://open-vsx.org/api/zobo/php-intellisense/1.1.3/file/zobo.php-intellisense-1.1.3.vsix",
    "vscode-php-debug": "https://open-vsx.org/api/xdebug/php-debug/1.32.1/file/xdebug.php-debug-1.32.1.vsix",
    "plantuml": "https://open-vsx.org/api/jebbs/plantuml/2.17.5/file/jebbs.plantuml-2.17.5.vsix",
